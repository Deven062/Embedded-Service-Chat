
<script type='text/javascript'>
	function initEmbeddedMessaging() {
		try {
			embeddedservice_bootstrap.settings.language = 'en_US'; // For example, enter 'en' or 'en-US'

			embeddedservice_bootstrap.init(
				'00DWr000000Z8ab',
				'IMT_Messaging_Service',
				'https://insuremytrip--dev.sandbox.my.site.com/ESWIMTMessagingService1726735914903',
				{
					scrt2URL: 'https://insuremytrip--dev.sandbox.my.salesforce-scrt.com'
				}
			);
		} catch (err) {
			console.error('Error loading Embedded Messaging: ', err);
		}
	};
</script>
<script type='text/javascript' src='https://insuremytrip--dev.sandbox.my.site.com/ESWIMTMessagingService1726735914903/assets/js/bootstrap.min.js' onload='initEmbeddedMessaging()'>
</script>
